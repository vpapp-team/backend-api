-- DEFINITION --
const SNOWFLAKE = new UTIL.snowflake({
  epoche: CONFIG.snowflake.epoche,
  datacenter: CONFIG.snowflake.datacenter,
  worker: CLUSTER.worker.workID,
});

-- README --
| snowflake | object | / | false | config for the snowflake generator |
| snowflake.epoche | number | `1515151515151` | true | number to offset all dates to lower there values |
| snowflake.datacenter | number | / | false | uuid of the datacenter |

-- CFG-LOADER --
if (!CFG.snowflake.hasOwnProperty('epoche')) CFG.snowflake.epoche = 1515151515151;

-- CONFIG --
"snowflake": {
  "datacenter": 1
}

